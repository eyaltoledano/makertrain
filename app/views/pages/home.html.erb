Params: <%= params.inspect %>
Session: <%= session[:user_id].inspect %>

<hr>

<% if session[:user_id] %>
  <h1><%= @user.name %></h1>
  <h2>Email: <%= @user.email %></h2>
  <h2>UID: <%= @user.uid %></h2>
  <img src="<%= @user.image %>">
  <br>
  <%= link_to "Logout", logout_path %>
<% else %>
  <%= link_to('Log in with GitHub', '/auth/github') %>
  <br>
  <%= link_to('Log in with ProductHunt', '/auth/producthunt') %>
<% end %>
