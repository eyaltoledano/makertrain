<!DOCTYPE html>
<html>
  <head>
    <title>Makertrain</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>



  </head>

  <body class="">
    <% if logged_in? %>
      <%= render 'layouts/logged_in_navigation' %>
      <%= flash_notices %>
    <% else %>
      <%= render 'layouts/logged_out_navigation' %>
      <%= flash_notices %>
    <% end %>

    <div>
      <%= yield %>
    </div>

  </body>
</html>
